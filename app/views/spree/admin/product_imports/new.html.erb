<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @product_import } %>

<% content_for :page_title do %>
    <%= Spree.t(:new) + ' ' + Spree.t(:product_import) %>
<% end %>

<%= Spree::ProductImport.image_url_base %>

<%= form_for( [:admin, @product_import], html: { multipart: true } )do |f| %>
    <fieldset data-hook="new_product_import">
      <%= render partial: 'form', locals: { f: f } %>
    </fieldset>
    <div class="form-actions">
      <%= render partial: 'spree/admin/shared/new_resource_links' %>
    </div>
<% end %>


<%#= link_to 'Back', admin_product_imports_path %>
